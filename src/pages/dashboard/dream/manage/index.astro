---
import Layout from '@layouts/Layout.astro';

import ManageStories from '@components/dashboard/dream/ManageStories.svelte';

const { topic } = Astro.params;

const arrow = true;

const meta = {
  header: 'Manage Stories',
};

if (!topic) {
  Astro.redirect('/');
}
---

<Layout {...meta} {arrow}>
  <ManageStories client:load />
</Layout>
