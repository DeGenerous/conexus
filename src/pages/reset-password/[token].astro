---
import Layout from '@layouts/Layout.astro';
import ResetPassword from '@components/routes/ResetPassword.svelte';

const { token } = Astro.params;

const arrow = true;

const meta = {
  title: 'Welcome to CoNexus',
  description: 'Explore the best stories in the multiverse',
  image: '/favicon.ico',
  url: 'https://conexus-v1-test.degenerousdao.com/',
};
---

<Layout {...meta} {arrow}>
  <ResetPassword {token} client:load />
</Layout>
